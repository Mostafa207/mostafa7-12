<Page x:Class="me.profile"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:me"
      mc:Ignorable="d" 
      Background="White"
      d:DesignHeight="550" d:DesignWidth="800"
      Title="profile">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="96*"/>
            <RowDefinition Height="173"/>
            <RowDefinition Height="281*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Label x:Name="lable" Content="" HorizontalAlignment="Left" Margin="244,31,0,0" VerticalAlignment="Top" Height="48" Width="247" HorizontalContentAlignment="Right" FontSize="30" Background="#FFEDDADA"/>
        <Button Content="log out" HorizontalAlignment="Left" Margin="32,113,0,0" Grid.Row="1" VerticalAlignment="Top" Height="48" Width="157" FontSize="20" RenderTransformOrigin="0.322,-2.486" Click="Button_Click"/>
        <DataGrid x:Name="grid" Grid.Row="2" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,7,0,0" SelectionChanged="DataGrid_SelectionChanged"/>



    </Grid>
</Page>
