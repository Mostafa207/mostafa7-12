<Page x:Class="me.search"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:me"
      mc:Ignorable="d" 
      Background="White"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="search">

    <Grid>
        <TextBox x:Name="txtc" HorizontalAlignment="Left" Margin="243,28,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Style="{StaticResource txtbox}"/>
        <Button Content="search" HorizontalAlignment="Left" Margin="615,28,0,0" VerticalAlignment="Top" Style="{StaticResource button}" Click="Button_Click"/>
        <Label Content="city" HorizontalAlignment="Left" Margin="87,31,0,0" VerticalAlignment="Top" Style="{StaticResource lable}"/>
        <DataGrid x:Name="grid"  Margin="0,181,0,0"/>

    </Grid>
</Page>
